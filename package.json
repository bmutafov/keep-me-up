{
  "name": "keep-me-up",
  "version": "1.2.0",
  "description": "A simple way to keep your PC awake with no configuration and setup.",
  "main": "index.js",
  "homepage": "https://github.com/mutafow/keep-me-up#readme",
  "scripts": {
    "start": "node index.js",
    "publish-npm": "npm publish --access public",
    "build": "pkg ."
  },
  "bin": "index.js",
  "keywords": [
    "awake",
    "keep pc awake",
    "mouse mover",
    "lightweight",
    "simple awake"
  ],
  "author": "bmutafov",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/mutafow/keep-me-up"
  },
  "dependencies": {
    "@nut-tree/nut-js": "^2.0.1",
    "desktop-idle": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.16",
    "eslint": "^8.10.0"
  },
  "pkg": {
    "scripts": "index.js",
    "assets": [
      "package.json",
      "node_modules/desktop-idle",
      "node_modules/@nut-tree"
    ],
    "targets": [
      "node16-macos",
      "node16-linux",
      "node16-win"
    ],
    "outputPath": "build",
    "compress": "GZip"
  }
}
